            			function foldSpecialSearch(){
            				var eVariant = $('special_search');
            				
            				if(!daum.Element.hasClassName(eVariant, "on_special_search")){
                    	   		daum.Element.addClassName(eVariant, "on_special_search");
                    	   	} else{
                    	   		daum.Element.removeClassName(eVariant, "on_special_search");
                    	   	}
							
							if($('layerHandWrite') && daum.Element.visible($('layerHandWrite'))){
                				daum.Element.hide($('layerHandWrite'));
                			}
							if($('layerLetter') && daum.Element.visible($('layerLetter'))){
                				daum.Element.hide($('layerLetter'));
                			}					
            			}
