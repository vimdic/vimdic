<script type="text/javascript">

	function checkSubmit(form) {
		//function(form){window.location.href = "http://m.search.daum.net/mobile/search?q="+form.query.value+"&nil_profile=section&rtupcate=suggest_dic"} 
		if(daum.String.trim(daum.$('q').value) == ""){
			alert("검색어를 입력해주세요");
			daum.$('q').focus();
			return false;
		}
		var searchURL = "/search.do?";
		var dicType = form.type.value;
		var q = form.q.value;
		q = q.trim();
		q = encodeURIComponent(q);
		
		if(dicType == 'all'){ 
			searchURL += "q=" + q;
		}else{
			searchURL += "q=" + q + "&dic="+dicType;
		}
		location.href = searchURL;

		return false;
	}
	
	function goTotalSearch() {
		window.open("http://search.daum.net/search?q="+encodeURIComponent(document.getElementById('q').value));
	}
</script>


		






