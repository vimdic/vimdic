    (function() {
		var pageId = 'search';
    	var qTop = document.getElementById('q');
				//IE
    	if (qTop.createTextRange) {    
    		var FieldRange = qTop.createTextRange();  
    		FieldRange.moveStart('character', qTop.value.length);  
    		FieldRange.collapse();  
    	}  
    	else {    
    		qTop.setSelectionRange(qTop.value.length,qTop.value.length); 
    	}     
		
    	qTop.onfocus = function() {
		 	var _btn = $('mobileGnb');
  			var _gnb = $('daumGnb');
			
    		if(daum.Element.hasClassName($('daumGnb'), "daumgnb_on")){
    			daum.Element.replaceClassName(_btn,"gnb_on","gnb_off");
    			daum.Element.replaceClassName(_gnb,"daumgnb_on","daumgnb_off");	 
        	}
		
    		document.getElementById('searchBox').className += ' search_on';
    	};
    	qTop.onblur = function() {
    		document.getElementById('searchBox').className = 'box_search';
    	};
		if (DEVICE_TYPE === 'PC') {
			if (pageId === 'wrap_top') {
				qTop.focus();
			} else {
				qTop.blur(); // safari에서 검색창이 자동으로 focus 되는 경우가 있으므로, blur 를 호출해준다.
			}
		}
    })();
    </script>
